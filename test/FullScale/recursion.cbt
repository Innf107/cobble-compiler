--# linear recursion : score
gauss :: Int -> Int;
gauss n = if __le__ n 0 then 0 else __add__ n (gauss (__add__ n -1));

--! 3
gauss 2

--! 55
gauss 10

--# branch recursion : score
fib :: Int -> Int;
fib n = if __le__ n 1 then 1 else __add__ (fib (__add__ n -1)) (fib (__add__ n -2));

--! 89
fib 10