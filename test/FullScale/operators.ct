--# singleFixity : score

infixr 5 
(+) :: Int -> Int -> Int;
(+) = __add__;


infixl 5
(-) :: Int -> Int -> Int;
(-) = __sub__;

--! 5
4 + 1

--! 3
4 - 1

--! 6
1 + 2 + 3

--! 1
6 - 3 - 2

--# multiFixity : score

infixl 5
(-) :: Int -> Int -> Int;
(-) = __sub__;

infixr 4
(*) :: Int -> Int -> Int;
(*) = __mul__;

--! 50
100 - 10*3 - 4*(7 - 2)

--! 0
4 * 3 - 2*6
